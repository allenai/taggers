Name := RedefineTagger(Name)
AP := RedefineTagger(AP)
Rel := RedefineTagger(Rel)
RelTo := RedefineTagger(RelTo)

NP := TypePatternTagger {
   (?: (?: <pos='DT'> | <pos='PRP$'>)? @AP? <pos=/NN.*/>+ )
   | (?:@Name)
}

NG := TypePatternTagger {
   (?:@NP )
   | (?:@NP <string="of"> @NP )
   | (?:@NP (?:<string=","> @NP)* <string=",">? <string="and"> @NP )
   | (?:@NP <string="("> @NP <string=")"> )
   | (?: <pos='PRP'> )
}
