Name := RedefineTagger(Name)
NG := RedefineTagger(NG)
PP := RedefineTagger(PP)
Rel := RedefineTagger(Rel)
RelTo := RedefineTagger(RelTo)

Modal := PatternTagger {
  <pos="MD"> <pos="RB">? 
}
Aux := PatternTagger {
  <string=/be|am|is|are|have|has/> <pos="RB">? <string="been">?
}
VG := TypePatternTagger {
